import random

# Generate random numbers from dices
class doceRoll:
        d4 = random.randint(1, 4),
        d6 = random.randint(1, 6),
        d8 = random.randint(1, 8),
        d10 = random.randint(1, 10),
        d12 = random.randint(1, 12),
        d20 = random.randint(1, 20),
        d100 = random.randint(1, 100)
